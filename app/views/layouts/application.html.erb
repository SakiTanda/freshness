<!DOCTYPE html>
<html>
  <head>
    <title>Freshness</title>
    <%= csrf_meta_tags %>
    <%= display_meta_tags(default_meta_tags) %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>

    <!-- google API -->
    <script async defer src="https://maps.googleapis.com/maps/api/js?key=AIzaSyDgMu1uClEjQXzCjz_JZ-o9sfbbT9kWnoQ&callback=initMap" type="text/javascript"></script>
    <script src="//maps.google.com/maps/api/js?key=AIzaSyDgMu1uClEjQXzCjz_JZ-o9sfbbT9kWnoQ"></script>
    <script src="//cdn.rawgit.com/mahnunchik/markerclustererplus/master/dist/markerclusterer.min.js"></script>
    <script src='//cdn.rawgit.com/printercu/google-maps-utility-library-v3-read-only/master/infobox/src/infobox_packed.js' type='text/javascript'></script> <!-- only if you need custom infobox -->

    <!-- specific css --> 
    <%= stylesheet_link_tag params[:controller] %> 

    <!-- specific js --> 
    <%= javascript_include_tag params[:controller] %> 
  </head>

  <body>
    <%= yield %>
  </body>
</html>
